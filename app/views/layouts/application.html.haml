!!!
%html#html-top
  %head
    %title NATHAN MCGARIGAL
    = stylesheet_link_tag    "application", media: "all"
    = stylesheet_link_tag    "http://fonts.googleapis.com/css?family=Lato:300,400,700,900,300italic", media: "all"
    
  
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body{:class => "#{action_name}"}
    - if on_detail_page?
      #detail-page.clearfix
        = render "shared/detail_page_menu"
        = yield
    - else
      = render "shared/primary_menu"
      = yield
      = render "shared/footer"
      